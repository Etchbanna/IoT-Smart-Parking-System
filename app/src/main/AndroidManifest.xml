        <activity
            android:name=".WadiActivity"
            android:exported="true" >
        </activity>
        <activity
            android:name=".P90Activity"
            android:exported="true" >
        </activity>
        <activity
            android:name=".BankActivity"
            android:exported="true" >
        </activity>

        <activity
            android:name=".ParkingLayoutActivity"
            android:exported="true">
